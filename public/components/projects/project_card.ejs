<div class="card" style="min-width: 300px; width: 300px;">
  <img
    class="card-img-top"
    src="../../images/stock.jpg"
    alt="Card image"
    style="width: 100%"
  />
  <div class="card-body d-flex flex-column">
    <a href="../../project" class="btn btn-dark"><% if (locals.projectName) {%><%- projectName %><% } %></a><br />
    <a href="../../team" class="link-dark text-decoration-none mx-auto" style="width: fit-content;">
      <i class="bi bi-people-fill"></i>
      <% if (locals.teamName) {%><%- teamName %><% } %>
    </a>
  </div>
</div>
